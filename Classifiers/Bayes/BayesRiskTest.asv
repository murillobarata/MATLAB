function output = BayesRiskTest( x, modelo, riskMatrix )
%BAYESRISKTEST Summary of this function goes here
%   Detailed explanation goes here

    for j = 1:length(riskMatrix)
        out = [];
        for k = 1:length(riskMatrix)
            out(k, :) = (riskMatrix(k,j) * mvnpdf(x, modelo.media(:, :, j), modelo.matCov(:, :, j))') * ... 
                modelo.apriori(j);
        end
        loss(j,:) = sum(out);
    end        
        
    [output.valores, output.classes] = min(loss);

end

